{
  "name": "resume-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "serve:ssr:resume-website": "node dist/resume-website/server/server.mjs",
    "optimize-images": "node scripts/optimize-images.js",
    "optimize-single": "node scripts/optimize-images.js",
    "emulators:hosting": "firebase emulators:start --only hosting",
    "emulators:functions": "firebase emulators:start --only functions",
    "build-all:emulators": "ng build --configuration development && cd functions && tsc && cd ../ && firebase emulators:start",
    "emulator": "firebase emulators:start --inspect-functions",
    "server": "cd functions && tsc --watch",
    "client": "ng build --watch --configuration development"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.4",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "ngx-extended-pdf-viewer": "^24.2.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "sharp": "^0.34.3",
    "typescript": "~5.8.2"
  }
}
